ps://cdn.shopify.com/s/files/1/0936/5772/7306/files/avant_apres_video_1mp4.webp?v=1749429320\">\n        <source src=\"https://cdn.shopify.com/videos/c/o/v/428983a1d6aa447ba3136b25214989a9.mp4\" type=\"video/mp4\">\n      </video>\n    </div>\n  </div>\n\n  <div class=\"video-pagination\" id=\"videoPagination\">\n    <div class=\"video-dot active\"></div>\n    <div class=\"video-dot\"></div>\n    <div class=\"video-dot\"></div>\n  </div>\n</div>\n\n<script>\n  const carousel = document.getElementById('videoCarousel');\n  const dots = document.querySelectorAll('.video-dot');\n\n  function updateActiveDot() {\n    const scrollLeft = carousel.scrollLeft;\n    const width = carousel.offsetWidth;\n    const index = Math.round(scrollLeft / width);\n    dots.forEach((dot, i) => {\n      dot.classList.toggle('active', i === index);\n    });\n  }\n\n  carousel.addEventListener('scroll', () => {\n    if (window.innerWidth < 768) {\n      updateActiveDot();\n    }\n  });\n</script>",
